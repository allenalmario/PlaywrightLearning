<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Functional Suite">
    
    <listeners>
        <listener class-name="section12.customListeners.Listeners"></listener>
    </listeners>
    <test name="Login Test">
        <classes>
            <class name="section12.TestNGAnnotations"/>
        </classes>
    </test>
    <test name="Validate Title Test">
        <classes>
            <class name="section12.Assertions"/>
        </classes>
    </test>
    <test name="Dependency Test">
        <classes>
            <class name="section12.TestDependencies"/>
        </classes>
    </test>
    <test name="Failure Test">
        <classes>
            <class name="section12.TestDependencies"/>
        </classes>
    </test>
</suite>